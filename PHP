function searchValueInAssocArray($array, $valueToSearch) {
 $foundKeys = array_keys($array, $valueToSearch); // Get all keys with the value
 if (!empty($foundKeys)) {
 return $foundKeys;
 } else {
 return false; // Value not found
 }
}
