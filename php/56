 [1] => banana
 [2] => cherry
 [3] => mango
)
Array
(
 [0] => mango
 [1] => cherry
 [2] => banana
 [3] => apple
)
Array
(
 [banana] => 20
 [mango] => 40
 [apple] => 50
)
